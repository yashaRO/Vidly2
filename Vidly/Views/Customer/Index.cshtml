@{
    ViewBag.Title = "Customer";
}
<h1>@ViewBag.Title.</h1>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>@ViewBag.Title</th>
            <th>Discount Rate</th>
        </tr>
    </thead>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@Html.ActionLink((string) customer.Name, "Details", "Customer", new {id = customer.Id}, null)</td>
            <td>@customer.MembershipType.Name</td>
        </tr>
    }

</table>