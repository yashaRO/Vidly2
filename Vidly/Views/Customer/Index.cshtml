@{
    ViewBag.Title = "Customer";
}
<h1>@ViewBag.Title.</h1>

<table>
    <tr>
        <thead>
            @ViewBag.Title
        </thead>
    </tr>
    @foreach (var customer in Model)
    {
        <tr>
            <td>@Html.ActionLink((string)customer.Name, "Details", "Customer", new { id = customer.Id}, null)</td>
        </tr>
    }

</table>